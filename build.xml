<?xml version="1.0" encoding="UTF-8"?>
<project name="hammingnumber" default="compile" basedir=".">
	<description>
		Ant build file for  hammingnumber
	</description>
	
	<target name="init">
		<tstamp/>
		<mkdir dir="bin"/>
	</target>

	<target name="compile" depends="init" description="compile the source" >
		<javac includeantruntime="false" srcdir="myproject" destdir="bin" classpath = "junit.jar" debug="on">
        </javac>
    </target>
   	<target name="clean">
		<delete dir="bin"/>
	</target>
	
</project>
